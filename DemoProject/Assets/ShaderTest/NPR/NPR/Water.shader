			o.Emission = tex2D( _ReflectionTex, (ase_screenPosNorm).xyzw.xy ).rgb;
			o.Alpha = 1;
		}

		ENDCG
	}
	Fallback "Diffuse"
	CustomEditor "ASEMaterialInspector"
